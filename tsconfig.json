{
    "compilerOptions": {
      "target": "ES2015",
      "module": "ES2015",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "declaration": true,
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "dist/**/*"
    ],
}
